type Answer{
    id: ID!,
    value: String
}

type Question{
    id: ID!,
    value: String,
    answers: [Answer]
}

type Survey{
    id: ID!,
    name: String,
    questions: [Question]
}

type Query{
    getQuestions: [Question]
    getAnswers: [Answer]
    getSurveys: [Survey]
    getSurveyByName(name: String): Survey
}